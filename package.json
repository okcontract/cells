{
  "name": "@okcontract/cells",
  "version": "0.1.7",
  "description": "Functional & Reactive Programming library inspired by spreadsheets",
  "private": false,
  "main": "dist/cells.umd.cjs",
  "module": "dist/cells.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/cells.js",
      "require": "./dist/cells.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist/", "assets/", "README.md", "LICENSE"],
  "dependencies": {
    "@okcontract/graph": "0.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.11.5",
    "happy-dom": "13.0.2",
    "immer": "^10.0.3",
    "terser": "5.26.0",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vitest": "1.1.3"
  },
  "scripts": {
    "build": "npm run format && vite build",
    "test": "vitest run",
    "definitions": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm test && npm run build && npm run check && npm run definitions",
    "check": "npx @biomejs/biome check src",
    "format": "npx @biomejs/biome format src --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okcontract/cells.git"
  },
  "author": "Henri Binsztok",
  "license": "MIT"
}
